{
"manifest_version": 3,
    "name": "Google Chat thread links & quote reply (mod)",
    "description": "Adds button to copy links to threads on Google Chat and adds button to messages to quote reply",
    "version": "0.0.12",
    "content_scripts": [
        {
            "matches": [
                "https://chat.google.com/*",
                "https://mail.google.com/chat/*"
            ],
            "all_frames": true,
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}